{% block htmlPage %}
  {% include '_inc/_scripts.twig' %}

    {% block siteHead %}
    <!DOCTYPE html>
    <html lang="en-US">
        <head>
            <meta charset="utf-8">
            <meta content="width=device-width, initial-scale=1" name="viewport">
            <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
            <meta http-equiv="X-UA-Compatible" content="IE=edge">

            {# Preload fonts #}
            <link rel="preload" as="font" type="font/woff2" href="{{ siteUrl }}dist/fonts/TitilliumWeb-Regular.woff2" crossorigin="anonymous">
                    
            {% hook "seo" %}
            {% block headStyles %}
            <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
            <link rel="stylesheet" href="{{ siteUrl }}dist/main.css">
            <link href="{{ siteUrl }}assets/images/favicon.ico" rel="shortcut icon" type="image/x-icon">
            <link href="{{ siteUrl }}assets/images/webclip.png" rel="apple-touch-icon">
            {% endblock %}
            {% block headScripts %}
            {% endblock %}
        </head>
        {% endblock %}
        {% block body %}
        <body>
            <a class="skip-link screen-reader-text" href="#nav-main">Skip to navigation</a>
            <a class="skip-link screen-reader-text" href="#content">Skip to content</a>
            <div class="site-container">
            {% block siteHeader %}
            <header class="site-header container navbar navbar-expand-md navbar-dark bg-transparent">
                <div class="header--wrap container gx-5 ">
                    <a 
                    aria-label="Return to Home Page"
                    href="{{ siteUrl }}"
                    class="logo--link navbar-brand">
                    {% set logo = branding.orgLogo.one() %}
                        {% if logo and logo.extension == 'svg' %}
                        {{ svg(logo)|attr({width: '188', class: 'logo'}) }}
                        {% endif %}
                        {% if logo and logo.extension != 'svg' %}
                        <img src="{{ logo.url }}" id="logo" class="logo" alt="{{ branding.orgName|e }} Logo">
                    {% endif %}
                    </a>
                    {# Navbar #}
                    <button id="navbar-toggler" class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#nav-main" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation"><span class="navbar-toggler-icon"></span></button>
                    <nav id="nav-main" class="main-menu collapse navbar-collapse container-fluid gx-5">
                        <ul class="nav--list navbar-nav mx-auto">
                            <li><a class="nav-link" href="#">About</a></li>
                            <li><a class="nav-link" href="#">Speakers</a></li>
                            <li><a class="nav-link" href="#">Schedule</a></li>
                        </ul> 
                        <a class="nav-link button" href="#">Register</a>
                    </nav>
                </div>{# end .header--wrap #}
            </header>
            {% endblock %}
            <main id="content" class="content">
            {% block main %}
                <h1>No show you here. Sorry!</h1>
            {% endblock %}
            </main>
        {% block siteFooter %}
            {# {% cache %} #}
            <footer class="site-footer container gx-5">
                <div class="footer--wrap">
                    <div class="footer-area first">
                        <figure class="footer-logo" aria-hidden="true">
                        {% set logo = branding.footerLogo.one() %}
                        {% if logo and logo.extension == 'svg' %}
                            {{ svg(logo)|attr({width: '75'}) }}
                            {% endif %}
                            {% if logo and logo.extension != 'svg' %}
                            <img src="{{ logo.url }}" id="logo" alt="{{ branding.orgName|e }} Logo">
                        {% endif %}
                        </figure>
                    </div>
                    <div class="footer-area last">
                        <h2 class="h3">Connect and Share</h2>
                        <div class="social-icons"><a class="social-icon facebook" href="//facebook.com/digconference" rel="noopener nofollow" aria-label="Follow us on Facebook aka Meta">{% include "_svg/icon-facebook.twig" %}</a><a class="social-icon twitter" href="//twiter.com/digconference" rel="noopener nofollow" aria-label="Follow us on Twitter">{% include "_svg/icon-twitter.twig" %}</a>
                    </div>
                </div>
            </footer>
            {# {% endcache %} #}
        {% endblock %}
            </div>{# end .site-container #}
            <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
            {{ sprig.script }}
            <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->
        </body>
        {% endblock %}
    </html>
{% endblock %} {# end htmlPage #}